{
  "name": "gutenverse-themes-builder",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run start --prefix gutenverse-core & npm run start:js & npm run start:css",
    "dev": "npm run dev --prefix gutenverse-core && npm run start:dev && npm run build:css",
    "build": "npm run build --prefix gutenverse-core && npm run build:js && npm run build:css",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "cross-env NODE_ENV=development webpack watch --mode development",
    "fix": "npx eslint --fix ./src || echo \"Formatting done! Please fix the remaining warnings and errors\"",
    "start:js": "cross-env NODE_ENV=development webpack --config .config/webpack.config.dev.js --watch",
    "start:dev": "cross-env NODE_ENV=development webpack --config .config/webpack.config.dev.js",
    "build:js": "cross-env NODE_ENV=production webpack --config .config/webpack.config.dev.js",
    "start:css": "cross-env NODE_ENV=development gulp watch",
    "build:css": "cross-env NODE_ENV=production gulp build",
    "development": "gulp clean && npm run development --prefix gutenverse-core && npm run dev && gulp release && npm run pot && gulp zip",
    "production": "gulp clean && npm run production --prefix gutenverse-core && npm run build && gulp release && npm run pot && gulp zip",
    "pot": "wp i18n make-pot ./release/gutenverse-themes-builder ./release/gutenverse-themes-builder/languages/gutenverse-themes-builder.pot --domain=gtb"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Jegstudio/gutenverse-themes-builder.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@svgr/webpack": "^8.0.1",
    "@uiw/react-textarea-code-editor": "^2.1.1",
    "@wordpress/api-fetch": "^3.20.0",
    "@wordpress/babel-preset-default": "^7.18.0",
    "@wordpress/dom-ready": "^3.11.0",
    "@wordpress/i18n": "^4.28.0",
    "@wordpress/url": "^3.29.0",
    "classnames": "^2.3.2",
    "crypto-js": "^4.1.1",
    "crypto-random-string": "^3.3.0",
    "lodash": "^4.17.21",
    "postcss": "^8.4.23",
    "react": "^16.13.1",
    "react-color": "^2.19.3",
    "react-dom": "^16.13.1",
    "react-dropzone": "^14.2.3",
    "react-feather": "^2.0.10",
    "react-router-dom": "^6.9.0",
    "react-select": "^5.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.17.12",
    "@wordpress/scripts": "^12.6.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.3.0",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.4",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^6.0.1",
    "file-loader": "^6.2.0",
    "filemanager-webpack-plugin": "^6.1.7",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-postcss": "^9.0.1",
    "gulp-sass": "^5.1.0",
    "gulp-string-replace": "^1.1.2",
    "gulp-zip": "^5.1.0",
    "gutenverse-core": "file:gutenverse-core",
    "sass": "^1.62.1",
    "sass-loader": "^10.4.1",
    "style-loader": "^3.3.3",
    "webpack": "^5.84.1",
    "webpack-cli": "^5.1.1"
  }
}
