{
  "name": "gutenverse-themes-builder",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run start --prefix gutenverse-core & npm run start:js & npm run start:css",
    "dev": "npm run dev --prefix gutenverse-core && npm run start:dev && npm run build:css",
    "build": "npm run build --prefix gutenverse-core && npm run build:js && npm run build:css",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "cross-env NODE_ENV=development webpack watch --mode development",
    "fix": "npx eslint --fix ./src || echo \"Formatting done! Please fix the remaining warnings and errors\"",
    "start:js": "cross-env NODE_ENV=development webpack --config .config/webpack.config.dev.js --watch",
    "start:dev": "cross-env NODE_ENV=development webpack --config .config/webpack.config.dev.js",
    "build:js": "cross-env NODE_ENV=production webpack --config .config/webpack.config.dev.js",
    "start:css": "cross-env NODE_ENV=development gulp watch",
    "build:css": "cross-env NODE_ENV=production gulp build",
    "development": "gulp clean && npm run development --prefix gutenverse-core && npm run dev && gulp release && npm run pot && gulp zip",
    "production": "gulp clean && npm run production --prefix gutenverse-core && npm run build && gulp release && npm run pot && gulp zip",
    "pot": "wp i18n make-pot ./release/gutenverse-themes-builder ./release/gutenverse-themes-builder/languages/gutenverse-themes-builder.pot --domain=gutenverse-themes-builder"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Jegstudio/gutenverse-themes-builder.git"
  },
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=10.2.3"
  },
  "overrides": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@svgr/webpack": "^8.1.0",
    "@uiw/react-textarea-code-editor": "^3.0.2",
    "@wordpress/api-fetch": "^7.4.0",
    "@wordpress/babel-preset-default": "^8.4.0",
    "@wordpress/block-editor": "^13.4.0",
    "@wordpress/block-library": "^9.4.0",
    "@wordpress/components": "^28.4.0",
    "@wordpress/dom-ready": "^4.4.0",
    "@wordpress/format-library": "^5.4.0",
    "@wordpress/i18n": "^5.4.0",
    "@wordpress/url": "^4.4.0",
    "classnames": "^2.5.1",
    "crypto-js": "^4.2.0",
    "crypto-random-string": "^5.0.0",
    "lodash": "^4.17.21",
    "postcss": "^8.4.40",
    "react-color": "^2.19.3",
    "react-dropzone": "^14.2.3",
    "react-feather": "^2.0.10",
    "react-router-dom": "^6.25.1",
    "react-select": "^5.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-react": "^7.24.7",
    "@wordpress/scripts": "^28.4.0",
    "axios": "^1.7.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^7.0.4",
    "file-loader": "^6.2.0",
    "filemanager-webpack-plugin": "^8.0.0",
    "gulp": "^5.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-postcss": "^10.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-string-replace": "^1.1.2",
    "gulp-zip": "^6.0.0",
    "gutenverse-core": "file:gutenverse-core",
    "sass": "^1.77.8",
    "sass-loader": "^15.0.0",
    "style-loader": "^4.0.0",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "animejs": "^3.2.2",
    "gsap": "^3.12.5",
    "mathjs": "^13.0.3"
  }
}
